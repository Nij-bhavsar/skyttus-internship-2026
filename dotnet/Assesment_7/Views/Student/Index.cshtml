@model IEnumerable<Assesment_7.Models.Student>

<h2>Student List</h2>

<table class="table table-bordered">
    <tr>
        <th>Name</th>
        <th>Age</th>
        <th>Course</th>
    </tr>

    @foreach (var s in Model)
    {
        <tr>
            <td>@s.Name</td>
            <td>@s.Age</td>
            <td>@s.Course</td>
        </tr>
    }
</table>
