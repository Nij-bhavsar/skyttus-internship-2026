@model List<MvcDemo.Models.Student>
@await Html.PartialAsync("_StudentHeader")
@{
    ViewData["Title"] = "Student List";
}

<h2>Students</h2>

<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Course</th>
        <th>Field</th>
    </tr>

@foreach (var student in Model)
{
    <tr>
        <td>@student.Id</td>
        <td>@student.Name</td>
        <td>@student.Course</td>
        <td>@student.Field</td>
    </tr>
}
</table>
